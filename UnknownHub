loadstring(game:HttpGet("https://raw.githubusercontent.com/Pixeluted/adoniscries/main/Source.lua",true))()

task.wait(5)

script.Parent = game.StarterPlayer.StarterPlayerScripts

task.wait(1)

local Players = game:GetService("Players")

local function createGuiForPlayer(player)
    local playerGui = player:WaitForChild("PlayerGui")
    
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "TransformedGui"
    screenGui.Parent = playerGui
    
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0.5, 0, 0.5, 0)
    frame.Position = UDim2.new(0.25, 0, 0.25, 0)
    frame.Parent = screenGui
    
    local loadingFrame = Instance.new("Frame")
    loadingFrame.Size = UDim2.new(1, 0, 0.2, 0)
    loadingFrame.Position = UDim2.new(0, 0, 0, 0)
    loadingFrame.Parent = frame
    
    local uiCorner = Instance.new("UICorner")
    uiCorner.Parent = loadingFrame
    
    local descFrame = Instance.new("ScrollingFrame")
    descFrame.Size = UDim2.new(1, 0, 0.8, 0)
    descFrame.Position = UDim2.new(0, 0, 0.2, 0)
    descFrame.Parent = frame
    
    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(1, 0, 0.5, 0)
    textLabel.Position = UDim2.new(0, 0, 0, 0)
    textLabel.Text = "Description"
    textLabel.Parent = descFrame
    
    local textLabel1 = Instance.new("TextLabel")
    textLabel1.Size = UDim2.new(1, 0, 0.5, 0)
    textLabel1.Position = UDim2.new(0, 0, 0.5, 0)
    textLabel1.Text = "More Info"
    textLabel1.Parent = descFrame
end

Players.PlayerAdded:Connect(createGuiForPlayer)

for _, player in Players:GetPlayers() do
    createGuiForPlayer(player)
end



warn("UnknownHub.")
